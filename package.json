{
    "name": "nativescript-systemui",
    "version": "5.0.0",
    "scripts": {
        "tsc": "cp src/systemui.d.ts plugin && tsc -skipLibCheck -d",
        "build": " cp README.md plugin/ && rm -f .tsbuildinfo && npm run tsc && npm run build.esm && node ./build.esm.js",
        "build.esm": "tsc -skipLibCheck --target es2015 --module esnext --removeComments --outDir build/esm",
        "demo.ios": "pnpm i && pnpm run tsc && cd demo && tns run ios",
        "demo.android": "pnpm i && pnpm run tsc && cd demo && tns run android",
        "clean.demo": "rimraf demo/hooks demo/node_modules demo/platforms",
        "clean": "rimraf plugin/ios plugin/android plugin/**/*.d.ts plugin/**/*.js plugin/**/*.js.map plugin/node_modules plugin/package-lock.json",
        "plugin.watch": "pnpm run tsc -- -w",
        "publish": "pnpm run build && lerna publish",
        "commitmsg": "commitlint -e $GIT_PARAMS"
    },
    "devDependencies": {
        "@commitlint/cli": "^9.0.0",
        "@commitlint/config-conventional": "^9.0.0",
        "@types/node": "^14.0.4",
        "husky": "^4.2.5",
        "lerna": "^3.21.0",
        "nativescript-angular": "~8.21.0",
        "npm-watch": "^0.6.0",
        "prompt": "^1.0.0",
        "recursive-copy": "^2.0.10",
        "rimraf": "^3.0.2",
        "through2": "^3.0.1",
        "@nativescript/core": "6.5.1",
        "tns-platform-declarations": "^6.5.1",
        "tslint": "^6.1.2",
        "tslint-config-prettier": "^1.18.0",
        "tslint-config-standard": "^9.0.0",
        "typescript": "~3.9.3"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Akylas/nativescript-systemui.git"
    },
    "keywords": [
        "nativescript",
        "native",
        "typescript",
        "ios",
        "statusbar",
        "status bar"
    ],
    "contributors": [
        {
            "name": "Burke Holland",
            "email": "burkeholland@gmail.com",
            "url": "https://github.com/burkeholland"
        },
        {
            "name": "Brad Martin",
            "email": "bradwaynemartin@gmail.com",
            "url": "https://github.com/bradmartin"
        }
    ],
    "license": "MIT",
    "homepage": "https://github.com/Akylas/nativescript-systemui#readme",
    "bugs": {
        "url": "https://github.com/Akylas/nativescript-systemui/issues"
    },
    "commitlint": {
        "extends": [
            "@commitlint/config-conventional"
        ]
    }
}
