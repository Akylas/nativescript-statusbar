{
    "name": "nativescript-systemui",
    "version": "5.0.0",
    "scripts": {
        "tsc": "cp src/systemui.d.ts plugin && cp README.md plugin && tsc -skipLibCheck",
        "build": "pnpm i && pnpm run tsc",
        "demo.ios": "pnpm i && pnpm run tsc && cd demo && tns run ios",
        "demo.android": "pnpm i && pnpm run tsc && cd demo && tns run android",
        "clean.demo": "rimraf demo/hooks demo/node_modules demo/platforms",
        "clean": "rimraf plugin/ios plugin/android plugin/**/*.d.ts plugin/**/*.js plugin/**/*.js.map plugin/node_modules plugin/package-lock.json",
        "plugin.watch": "pnpm run tsc -- -w",
        "publish": "pnpm run build && lerna publish",
        "commitmsg": "commitlint -e $GIT_PARAMS"
    },
    "devDependencies": {
        "@angular/core": "~8.1.3",
        "@commitlint/cli": "^8.1.0",
        "@commitlint/config-conventional": "^8.1.0",
        "@types/node": "^12.6.8",
        "husky": "^3.0.2",
        "lerna": "^3.16.4",
        "nativescript-angular": "~8.0.3",
        "npm-watch": "^0.6.0",
        "prompt": "^1.0.0",
        "rimraf": "^2.6.3",
        "tns-core-modules": "6.0.3",
        "tns-platform-declarations": "^6.0.3",
        "tslint": "^5.18.0",
        "tslint-config-prettier": "^1.18.0",
        "tslint-config-standard": "^8.0.1",
        "typescript": "~3.5.3"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Akylas/nativescript-systemui.git"
    },
    "keywords": [
        "nativescript",
        "native",
        "typescript",
        "ios",
        "statusbar",
        "status bar"
    ],
    "contributors": [
        {
            "name": "Burke Holland",
            "email": "burkeholland@gmail.com",
            "url": "https://github.com/burkeholland"
        },
        {
            "name": "Brad Martin",
            "email": "bradwaynemartin@gmail.com",
            "url": "https://github.com/bradmartin"
        }
    ],
    "license": "MIT",
    "homepage": "https://github.com/Akylas/nativescript-systemui#readme",
    "bugs": {
        "url": "https://github.com/Akylas/nativescript-systemui/issues"
    },
    "commitlint": {
        "extends": [
            "@commitlint/config-conventional"
        ]
    }
}
